anch       INTEGER REFERENCES branch\n)ENGINE = InnoDB DEFAULT charset = utf8;\n\n# Ciselnik stavov uctu\nCREATE TABLE ref_account_state (\n  Id_state INTEGER PRIMARY KEY AUTO_INCREMENT,\n  Description   VARCHAR(30)\n)ENGINE = InnoDB DEFAULT charset = utf8;\n\n# Tabulka uctov\nCREATE TABLE account (\n  Account_num     VARCHAR(10) PRIMARY KEY,\n  Creation_date DATE,\n  ref_account_state      INTEGER REFERENCES ref_account_state,\n  Balance       INTEGER,\n  ID_client      INTEGER REFERENCES client,#vlastnik\n  ID_branch     INTEGER REFERENCES branch,\n  ID_employee INTEGER REFERENCES employee\n)ENGINE = InnoDB DEFAULT charset = utf8;\n\n# Tabulka disponentov - vazobna\nCREATE TABLE disponent (\n  ID_client   INTEGER,\n  Account_num  VARCHAR(10),\n  Limit_trans INTEGER,\n  Validity    DATE     #do\n)ENGINE = InnoDB DEFAULT charset = utf8;\n\nALTER TABLE disponent \n  ADD CONSTRAINT PK_disponent PRIMARY KEY (ID_client, Account_num);\n  \nALTER TABLE disponent \n  ADD CONSTRAINT FK_disponent_client FOREIGN KEY (ID_client)\n  REFERENCES client(ID_client) ;#ON DELETE CASCADE;\n\nALTER TABLE disponent \n  ADD CONSTRAINT FK_disponent_account FOREIGN KEY(Account_num) \n  REFERENCES account(Account_num) ;#ON DELETE CASCADE;\n\n# Ciselnik typov kariet\nCREATE TABLE ref_card_type(\n  Id_type INTEGER PRIMARY KEY AUTO_INCREMENT,\n  Description  VARCHAR(20)\n)ENGINE = InnoDB DEFAULT charset = utf8;\n\n# Ciselnik vydavatelov kariet\nCREATE TABLE ref_card_issuer(\n  Id_issuer INTEGER PRIMARY KEY AUTO_INCREMENT,\n  Name  VARCHAR(20)\n)ENGINE = InnoDB DEFAULT charset = utf8;\n\n# Tabulka kariet\nCREATE TABLE credit_card (\n  Card_num    VARCHAR(16) PRIMARY KEY,\n  Card_type            INTEGER,\n  Issuer      INTEGER,\n  Validity       DATE,\n  PIN            VARCHAR(4),\n  CVC            VARCHAR(3),\n  Limit_trans    INTEGER,\n  ID_client      INTEGER REFERENCES client,\n  Account_num     VARCHAR(10) REFERENCES account,\n  ID_employee INTEGER REFERENCES employee\n)ENGINE = InnoDB DEFA